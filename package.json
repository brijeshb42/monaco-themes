    "copyfiles": "^2.4.1",
    "follow-redirects": "^1.9.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "fast-plist": "^0.1.3"
  }
}
