{
  "name": "monaco-themes",
  "version": "0.4.5",
  "description": "Themes for monaco editor in web browser",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run build && npm run download && npm run generate",
